<% content_for :page_title do %>
  <%= link_to Spree.t(:import_document), spree.admin_imported_documents_path %> /
  <%= @imported_document.name %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @imported_document } %>

<%= form_for [:admin, @imported_document] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>
